<!-- HEADER -->
<%= render partial: "header", locals: {user: @user} %>

<!-- HOME -->
<div class="container" id="main">

  <div class="row">

    <%= render partial: "left", locals: {user: @user} %>

    <!-- MIDDLE COLUMN -->
